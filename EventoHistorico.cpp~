
#include <iostream>
#include "EventoHistorico.h"

using namespace std;

EventoHistorico::EventoHistorico(){
//constructor por defecto
	fecha = 1900;
	eventos = new string[];
}

EventoHistorico::EventoHistorico(int f, string e){
//constructor
	fecha = f;
	evento = e;
}

EventoHistorico::EventoHistorico(const EventoHistorico& c){
//constructor de copia
	fecha = c.fecha;
	evento = c.evento;
}

void EventoHistorico::asignar(int f, string e){
//asignaci√≥n
	fecha = f;
	evento = e
}

